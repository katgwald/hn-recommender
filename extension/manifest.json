{
    "manifest_version": 2,
    "name": "HN Detector",
    "version": "0.1",
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "content_scripts": [
        { 
        "matches": [
            "https://news.ycombinator.com/"
        ],
        "js": ["jquery-3.3.1.js", "content.js"]
        }
    ]
}
